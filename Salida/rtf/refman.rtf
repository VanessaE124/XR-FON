00009     {\cf17 public} Vector3 obtenerPosAbIzq;{\cf20 //Vector que se va a utilizar para almaceanr la posici\'F3n del segundo target, con sus coordenadas se har\'E1n los c\'E1lculos necesarios}\par
00010 \par
00011     {\cf17 public} {\cf18 float} Z; {\cf20 //Posicion de la mesa en el eje Z}\par
00012     {\cf17 public} {\cf18 float} X; {\cf20 //Posicion de la mesa en el eje X}\par
00013     {\cf17 public} {\cf18 float} Y; {\cf20 //Posicion de la mesa en el eje Y}\par
00014     {\cf17 public} {\cf18 float} SZ; {\cf20 //Escala dela mesa en el eje Z}\par
00015     {\cf17 public} {\cf18 float} SX; {\cf20 //Escala dela mesa en el eje X}\par
00016     {\cf17 public} {\cf18 float} SY; {\cf20 //Escala dela mesa en el eje Y}\par
00017 \par
00018     {\cf20 // Versi+on siguiente, esto no se est\'E1 usando actualmente en el c\'F3digo}\par
00019     Collider m_Collider;\par
00020     {\cf17 public} Vector3 boundPoint1;\par
00021     {\cf17 public} Vector3 boundPoint2;\par
00022     {\cf17 public} {\cf18 float} posx;\par
00023     {\cf17 public} {\cf18 float} posz;\par
00024 \par
00025     {\cf20 //Debe asegurarse de llenar estos campos en la ventana inspector}\par
00026     [SerializeField] {\cf18 string} NombreAbIzq; {\cf20 //Nombre del primer target (ImageAbIzq), este se debe ubicar en la esquina inferior izquierda de la mesa}\par
00027     [SerializeField] {\cf18 string} NombreArDer; {\cf20 //Nombre del segundo Image Target (ImageArDer), este se debe ubicar en la esquina superior derecha de la mesa}\par
00028 \par
00029     {\cf20 //Contenedores de informaci\'F3n de los Image targets }\par
00030     GameObject ContenedorAbIzq; {\cf20 //En este GameObject se almacenan los datos del primer target (ImageAbIzq).}\par
00031     GameObject ContenedorArDer; {\cf20 //En este GameObject se almacenan los datos del segundo target (ImageArDer).}\par
00032 \par
00033     {\cf18 void} Start()\par
00034     \{   {\cf20 //Al iniciar la ejecuci\'F3n se buscan los Image targets a trav\'E9s del nombre digitado en la ventana de inspector para almacenar sus datos en un contenedor.}\par
00035         {\cf20 //Esto nos permitir\'E1 relizar opceraciones m\'E1s adelante}\par
00036         ContenedorArDer = GameObject.Find(NombreArDer);\par
00037         ContenedorAbIzq = GameObject.Find(NombreAbIzq);\par
00038 \par
00039        {\cf20 //Obtener el colider puesto en el modelo 3d (ACTUALMENTE NO S\'C9 EST\'C1 USANDO)}\par
00040             {\cf20 /*m_Collider = GetComponent<Collider>();}\par
00041 {\cf20             boundPoint1 = m_Collider.bounds.min;}\par
00042 {\cf20             boundPoint2 = m_Collider.bounds.max;}\par
00043 {\cf20             posx = boundPoint2.x*2;}\par
00044 {\cf20             posz=  boundPoint2.z*2;*/}\par
00045 \par
00046     \}\par
