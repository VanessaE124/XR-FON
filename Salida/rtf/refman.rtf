00049     {\cf18 void} Update()\par
00050     \{\par
00051         {\cf20 //Llamado a la funcion  obtenerPosicion;}\par
00052         obtenerPosicion();\par
00053     \}\par
00054     {\cf17 public} {\cf18 void} obtenerPosicion()\par
00055     \{    {\cf20 //Esta funci\'F3n permite obtener el vector con la posicion de ambos targets}\par
00056             obtenerPosArDer = ContenedorArDer.transform.position;\par
00057             obtenerPosAbIzq = ContenedorAbIzq.transform.position;\par
00058         {\cf20 //Llamado a las  funciones que realizan calculos de escala y posici\'F3n del target}\par
