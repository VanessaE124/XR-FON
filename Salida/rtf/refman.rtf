00065         {\cf20 //Para realizar estas operaciones de forma correcta se debe tener en cuenta que el pivote de la mesa debe estar en el centro de la misma.}\par
00066             Z = (obtenerPosArDer.z)/2;\par
00067             X = (obtenerPosArDer.x)/2;\par
00068         {\cf20 //Para objeter la posici\'F3n en el eje Y se debe tener en cuenta la escala del modelo en terminos de la posici\'F3n en Unity: En el caso de un cubo es (1,1,1)}\par
00069         {\cf20 //Se toma la posicion en el eje Y del segundo target y se le resta el tama\'F1o dividido en 2, este valor se establece como negativo para transladar hacia abajo el objeto.}\par
00070             Y = (obtenerPosArDer.y-SY/2); \par
00071                 seteoPosicion();\par
00072     \}\par
00073     {\cf17 public} {\cf18 void} calculoEscala()\par
00074     \{   {\cf20 //Esta funcion toma las coordenadas X, Y y Z del segundo target para establecer la escala de la mesa.}\par
00075         {\cf20 //Para escalar la mesa de forma correcta su modelo debe tener una escala de (1,1,1) en terminos de coordenadas en Unity }\par
00076             SZ = (obtenerPosArDer.z); \par
00077             SX = (obtenerPosArDer.x);\par
00078             SY = 0.5f;\par
00079             seteoEscala();\par
00080     \}\par
00081 \par
